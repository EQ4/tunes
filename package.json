{
    "name": "tunes",
    "description": "HTML5 audio and video playlists",
    "version": "0.6.1",
    "homepage": "https://github.com/ryanve/tunes",
    "license": "MIT",
    "author": "Ryan Van Etten",
    "keywords": ["playlist", "audio", "video", "media", "browser"],
    "main": "./tunes.js",
    "ender": "noop",
    "repository": {
        "type": "git",
        "url": "https://github.com/ryanve/tunes.git"
    }, 
    "devDependencies": {
        "grunt-contrib-uglify": "~0.8.0",
        "grunt-contrib-concat": "~0.5.0",
        "grunt-contrib-jshint": "~0.11.0",
        "universal": "~0.1.0"
    },
  "jshintConfig": {
    "asi": false,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {
      "define": true,
      "ender": true 
    },
    "jquery": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "loopfunc": true,
    "maxcomplexity": 15,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 6,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}